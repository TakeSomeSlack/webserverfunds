<body>

<script>
function loadDoc() {
  const xhttp = new XMLHttpRequest();
  xhttp.open("POST", "log.php");
  xhttp.send();
}
</script>

<body>
    <form action="log.php" method="post" onsubmit="loadDoc()">
    <p>search: <input type="text" id="search" name="search"></p>
    <input type="submit" value="submit"/>
    </form>
</body>
</html>